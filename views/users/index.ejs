<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
  </head>
  <body>
    <h1><%= title %></h1>
    <form>
      <a href="/users/add" class="btn btn-default">Add</a>
    </form>

    <table class="table">
      <thead>
        <tr>
          <th>No</th>
          <th>EMAIL</th>
          <th>CREATED AT</th>
          <th>ACTION</th>
        </tr>
      </thead>
      <tbody>
        <% for (var i=0;i < users.length;i++) { %>
        <tr>
          <th scope="row"> <%= i + 1 %> </th>
          <td><%= users[i].email %></td>
          <td><%= users[i].createdAt %></td>
          <td><a href="/users/update/<%= users[i].id %>" class="btn btn-info">update</a>
             <a href="/users/delete/<%= users[i].id %>" class="btn btn-danger">delete</a> </td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
